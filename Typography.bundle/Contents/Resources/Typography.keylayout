	<actions>
		<!-- Analphabetic keys -->
		<action id="backtick">
			<when state="none" output="`" />
		</action>
		<action id="opt_backtick">
			<when state="none" output="" />
		</action>
		<action id="hyphen">
			<when state="none" output="-" />
			<when state="currency_n" next="currency_n_hyphen" />
			<when state="currency_n_hyphen" output="&#x20A6;" />
			<when state="currency_p" next="currency_p_hyphen" />
			<when state="currency_p_hyphen" output="&#x20B1;" />
			<when state="currency_w" next="currency_w_hyphen" />
			<when state="currency_w_hyphen" output="&#x20A9;" />
			<when state="currency_k" output="&#x20AD;" />
			<when state="dashes" output="&#x2011;" />
			<when state="dots" output="&#x2043;" />
			<when state="math" output="&#x2212;" />
			<when state="superscripts" output="&#x207B;" />
			<when state="s_superscripts" output="&#x207B;" next="s_superscripts" />
			<when state="subscripts" output="&#x208B;" />
			<when state="s_subscripts" output="&#x208B;" next="s_subscripts" />
		</action>
		<action id="sh_hyphen">
			<when state="none" output="_" />
		</action>
		<action id="opt_hyphen">
			<when state="none" next="dashes" />
		</action>
		<action id="equals">
			<when state="none" output="=" />
			<when state="currency_n" output="&#x20A6;" />
			<when state="currency_p" output="&#x20B1;" />
			<when state="currency_w" output="&#x20A9;" />
			<when state="math" output="&#x2248;" />
			<when state="superscripts" output="&#x207C;" />
			<when state="s_superscripts" output="&#x207C;" next="s_superscripts" />
			<when state="subscripts" output="&#x208C;" />
			<when state="s_subscripts" output="&#x208C;" next="s_subscripts" />
		</action>
		<action id="sh_equals">
			<when state="none" output="+" />
			<when state="math" output="&#xB1;" />
			<when state="superscripts" output="&#x207A;" />
			<when state="s_superscripts" output="&#x207A;" next="s_superscripts" />
			<when state="subscripts" output="&#x208A;" />
			<when state="s_subscripts" output="&#x208A;" next="s_subscripts" />
		</action>
		<action id="opt_equals">
			<when state="none" next="math" />
			<when state="math" output="&#x2260;" />
		</action>
		<action id="sh_opt_equals">
